{
  "name": "prettier-plugin-nunjucks",
  "version": "0.0.1",
  "dependencies": {
    "@babel/core": "^7.7.7",
    "array.prototype.flatmap": "^1.2.3",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "mustache": "^3.2.0",
    "nunjucks": "^3.1.7"
  },
  "description": "Nunjucks plugin for Prettier",
  "author": "Rhys LLoyd <justrhysism@gmail.com>",
  "license": "MIT",
  "main": "src",
  "files": [
    "src"
  ],
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "pretest": "npm run build-standalone",
    "test": "jest",
    "prepublishOnly": "npm run build-standalone",
    "prettier": "prettier --plugin=. --parser=nunjucks",
    "build-standalone": "rollup -c build/rollup.config.js"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.7.5",
    "codecov": "3.7.1",
    "eslint": "6.7.2",
    "eslint-config-prettier": "6.7.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": ">=1",
    "jest": "24.9.0",
    "jest-runner-eslint": "0.7.5",
    "jest-snapshot-serializer-raw": "^1.1.0",
    "lint-staged": ">=8",
    "prettier": "^1.19.0",
    "rollup": "^1.1.2",
    "rollup-plugin-alias": "^2.0.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-inject": "^3.0.1",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.1.0",
    "rollup-plugin-terser": "^5.1.1",
    "strip-ansi": "^6.0.0"
  },
  "peerDependencies": {
    "prettier": "^1.19.0"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest.*.config.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
